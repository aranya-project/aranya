{
	// The daemon's name.
	"name": "memberb",
	
	// The daemon's working directory.
	//
	// Must already exist.
	"work_dir": "out/memberb/",

	// Used to receive API requests from the user library client.
	"uds_api_path": "out/memberb/uds.sock",

	// The path where the daemon should write its PID file.
	"pid_file": "out/memberb/hub.pid",

	// Aranya sync server address.
	"sync_addr": "127.0.0.1:10005",
}
